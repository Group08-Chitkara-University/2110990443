	}
}



Odd Square



import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        for(int i=0;i<n;i++){
            int oddNum=2*i+1;
            for(int j=0;j<n;j++){
                System.out.print(oddNum);
                oddNum+=2;
                if(oddNum==2*n+1){
                    oddNum=1;
                }
            }
            System.out.println();
        }
	}
}



