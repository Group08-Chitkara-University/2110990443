ANSWER:


import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {


		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		
		for(int i=1;i<=n;i++) {
			for(int space=1;space<=(n-i);space++) {
				System.out.print(" ");
			}
			int k=i;
			for(int j=1;j<=i;j++) {
				System.out.print(k++);
			}
			k=k-2;
			for(int j=k;j>=i;j--) {
				System.out.print(j);
			}
			System.out.println();
		}

	}

}



